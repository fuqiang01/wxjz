.num-box{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    background: #f5f5f5;
    display: flex;
    align-items: center;
    justify-content: space-around;
}
.num-box image{
    width: 40rpx;
    vertical-align: middle;
}
.num-box text{
    margin-left: 20rpx;
    vertical-align: middle;
    color: #999;
}