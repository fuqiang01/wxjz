.wrapper{
  margin: 30rpx;
}
.item{
  position: relative;
  width: 100%;
  margin-bottom: 30rpx;
}
.item image{
  width: 100%;
  border-radius: 10rpx;
}
.item text{
  position: absolute;
  display: block;
  top: 50%;
  left: 50%;
  color: #fff;
  font-size: 50rpx;
  transform: translate(-50%, -50%);
}
.feedback{
  position: fixed;
  bottom: 100rpx;
  right: 50rpx;
  background: rgba(0,0,0,.3);
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.feedback image{
  width: 50%;
}