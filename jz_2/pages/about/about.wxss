.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 200rpx;
}
.logo{
    width: 200rpx;
    height: 200rpx;
    box-shadow: 0 0 20rpx #eee;
    border-radius: 20rpx;
}
.logo image{
    width: 100%;
}
.name{
    font-size: 32rpx;
    line-height: 80rpx;
}
.version{
    font-size: 26rpx;
    color: #aaa;
}